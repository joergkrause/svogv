{
  "name": "svogv",
  "version": "0.6.0",
  "description": "A decorator based approach for model driven forms and some time saving form widgets.",
  "main": "index.js",
  "scripts": {
    "clean": "npm run clean-lib && npm run clean-demo",
    "clean-lib": "rimraf dist/lib",
    "clean-demo": "rimraf dist/demo",
    "build": "npm run clean && npm run build-lib && npm run build-demo",
    "build-lib": "npm run clean-lib && webpack --config webpack.config.js --profile --bail",
    "build-demo": "npm run clean-demo && cd ./src/demo && webpack --config webpack.config.js --progress --profile --bail && cd ../../",
    "start": "npm run build && http-server ./dist/demo --gzip -p 3000 -o",
    "test": "gulp test",
    "lint": "tslint --project ./src/lib",
    "docs": "gulp docs",
    "codedoc": "./node_modules/.bin/compodoc -p src/lib/tsconfig-srcs.json",
    "setup": "npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joergkrause/svogv.git"
  },
  "keywords": [
    "Angular",
    "Reactive",
    "Forms",
    "Validation",
    "Viewmodel",
    "decorators"
  ],
  "author": "Joerg <IsAGeek> Krause",
  "license": "ISC",
  "engines": {
    "node": ">= 5.4.1 < 10"
  },
  "bugs": {
    "url": "https://github.com/joergkrause/svogv/issues"
  },
  "homepage": "https://github.com/joergkrause/svogv#readme",
