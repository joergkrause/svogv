{
  "name": "svogv-demo",
  "version": "0.11.0",
  "description": "A decorator based approach for model driven forms and some time saving form widgets.",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4200",
    "build": "cp README.md projects/svogv/README.md && npm run codedoc && ng build svogv && ng build",
    "prod": "npm run codedoc-docs && ng build svogv && ng build --prod",
    "test": "ng test",
    "lint": "ng lint > linter-warnings.txt",
    "e2e": "ng e2e",
    "codedoc": "compodoc -p ./projects/svogv/tsconfig.lib.json --output ./src/documentation"
  },
  "dependencies": {
    "@angular/common": "^11.0.2",
    "@angular/compiler": "^11.0.2",
    "@angular/core": "^11.0.2",
    "@angular/forms": "^11.0.2",
    "@angular/platform-browser": "^11.0.2",
    "@angular/platform-browser-dynamic": "^11.0.2",
    "@angular/router": "^11.0.2",
    "bootstrap": "^4.5.3",
    "core-js": "^3.8.0",
    "font-awesome": "^4.7.0",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.3",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^10.0.1",
    "@angular-devkit/build-angular": "~0.1100.2",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular/cli": "^11.0.2",
    "@angular/compiler-cli": "^11.0.2",
    "@angular/language-service": "^11.0.2",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "^3.6.2",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^14.14.10",
    "codelyzer": "^6.0.1",
    "http-server": "^0.12.3",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^11.0.3",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.3",
    "typescript": "~4.0.0"
  }
}
