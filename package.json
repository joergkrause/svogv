{
  "name": "svogv-demo",
  "version": "0.8.0",
  "description": "A decorator based approach for model driven forms and some time saving form widgets.",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4200",
    "build": "cp README.md projects/svogv/README.md && ng build svogv && ng build",
    "prod": "ng build svogv && ng build --prod && npm run codedoc-docs",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "codedoc": "npm run codedoc-dist && npm run codedoc-docs",
    "codedoc-docs": "compodoc -p ./projects/svogv/tsconfig.lib.json --output ./docs/documentation",
    "codedoc-dist": "compodoc -p ./projects/svogv/tsconfig.lib.json --output ./dist/svogv-demo/documentation"
  },
  "dependencies": {
    "@angular/common": "^8.1.0",
    "@angular/compiler": "^8.1.0",
    "@angular/core": "^8.1.0",
    "@angular/forms": "^8.1.0",
    "@angular/platform-browser": "^8.1.0",
    "@angular/platform-browser-dynamic": "^8.1.0",
    "@angular/router": "^8.1.0",
    "bootstrap": "^4.3.1",
    "core-js": "^3.2.1",
    "font-awesome": "^4.7.0",
    "rxjs": "^6.5.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^8.1.0",
    "@angular-devkit/build-angular": "^0.802.1",
    "@angular-devkit/build-ng-packagr": "^0.802.1",
    "@angular/cli": "^8.2.1",
    "@angular/compiler-cli": "^8.1.0",
    "@angular/language-service": "^8.1.0",
    "@compodoc/compodoc": "^1.1.9",
    "@types/jasmine": "^3.4.0",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^10.14.0",
    "codelyzer": "~5.1.0",
    "http-server": "^0.11.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.2.0",
    "karma-chrome-launcher": "~3.0.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^5.4.3",
    "ts-node": "~8.3.0",
    "tsickle": "^0.36.0",
    "tslib": "^1.10.0",
    "tslint": "~5.18.0",
    "typescript": "~3.5.3"
  }
}
