{
  "name": "svogv-demo",
  "version": "0.6.0",
  "description": "Demo app for SVOGV module.",
  "scripts": {
    "clean": "npm run clean-lib && npm run clean-demo",
    "clean-lib": "rimraf dist/lib",
    "clean-demo": "rimraf dist/demo",
    "build": "npm run build-lib && npm run build-demo",
    "build-lib": "npm run clean-lib && webpack --config webpack.config.module.js --progress --profile --bail",
    "build-demo": "npm run clean-demo && webpack --config webpack.config.demo.js --progress --profile --bail",
    "start": "npm run build && http-server ./dist/demo --gzip -p 3000 -o",
    "lint": "tslint --project ./src/lib",
    "setup": "npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joergkrause/svogv.git"
  },
  "keywords": [
    "Angular",
    "Reactive",
    "Forms",
    "Validation",
    "Viewmodel",
    "decorators"
  ],
  "author": "Joerg <IsAGeek> Krause",
  "license": "ISC",
  "engines": {
    "node": ">= 5.4.1 < 10"
  },
  "bugs": {
    "url": "https://github.com/joergkrause/svogv/issues"
  },
  "homepage": "https://github.com/joergkrause/svogv#readme",
  "dependencies": {
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "bootstrap": "^4.1.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.7",
    "font-awesome": "^4.7.0",
    "rxjs": "^6.3.2",
    "svogv": "file:../../dist/lib",
    "systemjs": "0.19.38",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "^6.2.2",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/platform-server": "^6.0.0",
    "@ngtools/webpack": "^6.2.2",
    "@types/core-js": "^2.5.0",
    "@types/glob": "^5.0.35",
    "@types/merge2": "0.0.28",
    "@types/minimist": "^1.1.28",
    "@types/node": "^10.10.1",
    "@types/rx": "^4.1.1",
    "copy-webpack-plugin": "^4.5.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "fs-extra": "^1.0.0",
    "highlight.js": "^9.12.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.10.0",
    "node-sass": "^4.9.3",
    "protractor": "^5.4.1",
    "sass": "^1.13.4",
    "ts-helpers": "^1.1.2",
    "ts-node": "^3.3.0",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2",
    "url-loader": "^1.1.1",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.0",
    "webpack-filter-warnings-plugin": "^1.2.0",
    "which": "^1.3.1"
  }
}
